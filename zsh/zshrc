# Set personal aliases, overriding those provided by oh-my-zsh libs,
# plugins, and themes. Aliases can be placed here, though oh-my-zsh
# users are encouraged to define aliases within the ZSH_CUSTOM folder.
# For a full list of active aliases, run `alias`.
#
# Example aliases
# alias zshconfig="mate ~/.zshrc"
# alias ohmyzsh="mate ~/.oh-my-zsh"
export URGE_DIR="~/Urge/go/src/gitlab.com/URGE/" 
alias urgemain="cd $URGE_DIR/Main"
alias urgeserver="cd $URGE_DIR/Main/server"
alias urgeclient="cd $URGE_DIR/Main/client"
alias urgeinfo="cd $URGE_DIR/Main/information-api"
alias urgeios="cd $URGE_DIR/iOS"

export SaveMyDay_DIR="~/Library/Mobile\ Documents/com\~apple\~CloudDocs/SaveMyDay/"
alias smdweb="cd $SaveMyDay_DIR/Hjemmeside"

export PRIVATE_DIR="~/Google\ Drive/SaveMyDay/Opgaver/Mathias\ Egekvist"
alias gotomathias="cd $PRIVATE_DIR"

export NVM_DIR="$HOME/.nvm"
[ -s "$NVM_DIR/nvm.sh" ] && \. "$NVM_DIR/nvm.sh"  # This loads nvm
[ -s "$NVM_DIR/bash_completion" ] && \. "$NVM_DIR/bash_completion"  # This loads nvm bash_completion
export PATH="$PATH:/opt/yarn-[version]/bin"

export GOPATH=$HOME/Urge/go/
export PATH=$GOPATH/bin:$PATH

export PATH="$PATH:$(brew --prefix)/bin"
export PATH="/Library/Frameworks/Python.framework/Versions/3.6/bin:${PATH}"
# Add RVM to PATH for scripting. Make sure this is the last PATH variable change.
export PATH="$PATH:$HOME/.rvm/bin"
